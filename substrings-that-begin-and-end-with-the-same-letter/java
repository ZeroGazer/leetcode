class Solution {
    public long numberOfSubstrings(String s) {
        long[] freq = new long[26];
        for (char c : s.toCharArray()) {
            freq[c - 'a']++;
        }
        long count = 0;
        for (long i : freq) {
            System.out.println(i);
            count += (i + 1) * i / 2;
        }
        return count;
    }
}