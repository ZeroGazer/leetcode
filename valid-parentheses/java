import java.util.Stack;

class Solution {
    public boolean isValid(String s) {
        Stack<Character> stack = new Stack<>();
        Map<Character, Character> cm = Map.of(')', '(', '}', '{', ']', '[');
        
        for (int i = 0; i < s.length(); i++) {
            char c = s.charAt(i);
            if (!stack.isEmpty() && cm.get(c) == stack.peek()) {
                stack.pop();
            } else {
                stack.push(c);
            }
        }
        
        return stack.size() == 0;
    }
}