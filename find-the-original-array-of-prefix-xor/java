class Solution {
    public int[] findArray(int[] pref) {
        int[] result = new int[pref.length];
        int prev = 0;
        for (int i = 0; i < pref.length; i++) {
            int e = pref[i];
            result[i] = e ^ prev;
            prev ^= result[i];
        }
        return result;
    }
}